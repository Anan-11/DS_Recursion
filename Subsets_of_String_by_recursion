
public class Subsets {

    public static void subset(String s,String s1)
    {
        if(s.length()==0)
        {
            System.out.println(s1);
            return;
        }
        subset(s.substring(1),s1);
        s1=s1+s.charAt(0);

        subset(s.substring(1),s1);


    }


    public static void main(String[] args) {

        String s="abc";
        subset(s,"");
    }
}
